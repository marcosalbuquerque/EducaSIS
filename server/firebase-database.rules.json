{
  "rules": {
    ".read": "auth != null",
    ".write": "auth != null",
    "alunos": {
      ".indexOn": ["email"]
    },
    "professores": {
      ".indexOn": ["email"]
    },
    "tarefas": {
      ".indexOn": ["titulo", "descricao", "dataInicio", "dataFim"]
    },
    "entregas": {
      ".indexOn": ["id_tarefa", "id_aluno"]
    }
  }
}